{
   "apiVersion": "v1",
   "kind": "PersistentVolumeClaim",
   "metadata": {
       "name": "wordpress-pvclaim",
       "labels": {
           "deploymentconfig": "wodpress-openshift"
       }
   },
   "spec": {
       "accessModes": [ "ReadWriteMany" ],
       "resources": {
           "requests": {
               "storage": "1Gi"
           }
       }
   }
}
